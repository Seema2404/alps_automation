    "cypress-iframe": "^1.0.1",
    "cypress-real-events": "^1.7.0",
    "cypress-xpath": "^1.6.2",
    "faker": "^4.1.0",
    "fs": "0.0.1-security",
    "mocha": "^8.4.0",
    "mocha-junit-reporter": "^2.0.0",
    "mocha-simple-html-reporter": "^2.0.0",
    "mochawesome": "^6.3.1",
    "mochawesome-merge": "^4.1.0",
    "mochawesome-report-generator": "^5.2.0",
    "moment": "^2.26.0",
    "npm-run-all": "^4.1.5",
    "superagent": "^6.1.0",
    "supertest": "^6.1.3",
    "testmail": "^1.0.0"
  },
  "devDependencies": {
    "@types/faker": "^4.1.12",
    "@types/mochawesome": "^6.2.0",
    "@types/node": "^14.0.12",
    "@typescript-eslint/eslint-plugin": "^3.2.0",
    "@typescript-eslint/parser": "^3.2.0",
    "cy-verify-downloads": "0.0.5",
    "cypress-file-upload": "^5.0.8",
    "cypress-multi-reporters": "^1.5.0",
    "cypress-select-tests": "^1.7.1",
    "cypress-xpath": "^1.6.2",
    "eslint": "^7.2.0",
    "eslint-plugin-chai-friendly": "^0.6.0",
    "eslint-plugin-cypress": "^2.11.1",
    "eslint-plugin-filenames": "^1.3.2",
    "eslint-plugin-import": "^2.21.1",
    "typescript": "^3.9.5"
  },
  "linux": {
    "clean:reports": "rm -rf cypress/reports/* && mkdir -p cypress/reports/mochareports && mkdir -p cypress/reports/mochareports/mochawesome",
    "scripts": "cypress run",
    "combine-reports": "mochawesome-merge  cypress/reports/mocha/*.json > cypress/reports/mochareports/mochawesome/report.json",
    "generate-report": "marge cypress/reports/mochareports/mochawesome/report.json -f ui-tests -o cypress/reports/mochareports/mochawesome -i true",
    "report:copyScreenshots": "cp -R cypress/screenshots cypress/reports/mochareports/mochawesome/screenshots || :",
    "report:copyVideos": "cp -R cypress/videos cypress/reports/mochareports/mochawesome/videos || :",
    "test": "npm-run-all clean:reports scripts combine-reports generate-report report:copyScreenshots report:copyVideos --continue-on-error",
    "open": "npx cypress open",
    "lint": "eslint --ext ts .",
    "lint:fix": "npm run lint -- --fix"
  },
  "scripts_old": {
    "clean:reports": "rmdir /S /Q cypress\\reports && mkdir cypress\\reports && mkdir cypress\\reports\\mochareports",
    "scripts": "cypress run",
    "script-sanity": "cypress run --spec cypress/integration/sanity/*",
    "combine-reports": "mochawesome-merge cypress\\reports\\mocha\\*.json > output.json",
    "generate-report": "marge --reportDir cypress\\reports\\mochareports -t Cypress_Test_Report -p Cypress_HTML_Report -f Cypress_HTML_Report --charts true output.json",
    "report:copyScreenshots": "xcopy cypress\\screenshots cypress\\reports\\mochareports\\screenshots /E /H /C /I /Y",
    "report:copyVideos": "xcopy cypress\\videos cypress\\reports\\mochareports\\videos /E /H /C /I /Y",
    "test": "npm-run-all clean:reports scripts combine-reports generate-report report:copyScreenshots --continue-on-error",
    "post-run": "npm-run-all combine-reports generate-report report:copyScreenshots --continue-on-error",
    "sanity": "npm-run-all clean:reports script-sanity combine-reports generate-report report:copyScreenshots --continue-on-error",
    "lint": "eslint --ext ts .",
    "lint:fix": "npm run lint -- --fix"
  },
  "scripts": {
    "clean:reports": "rmdir /S /Q cypress\\reports && mkdir cypress\\reports && mkdir cypress\\reports\\mochareports",
    "scripts": "cypress run",
    "script-basic-sanity": "cypress run --spec cypress/integration/sanity/user-flows/**/*",
    "combine-reports": "mochawesome-merge cypress\\reports\\mocha\\*.json > output.json",
    "generate-report": "marge --reportDir cypress\\reports\\mochareports -t Cypress_Test_Report -p Cypress_HTML_Report -f Cypress_HTML_Report --charts true output.json",
    "report:copyScreenshots": "xcopy cypress\\screenshots cypress\\reports\\mochareports\\screenshots /E /H /C /I /Y",
    "report:copyVideos": "xcopy cypress\\videos cypress\\reports\\mochareports\\videos /E /H /C /I /Y",
    "test": "npm-run-all clean:reports scripts combine-reports generate-report report:copyScreenshots --continue-on-error",
    "test-basic-sanity": "npm-run-all clean:reports script-basic-sanity combine-reports generate-report report:copyScreenshots --continue-on-error",
    "lint": "eslint --ext ts .",
    "lint:fix": "npm run lint -- --fix"
  },
  "name": "ui",
  "version": "1.0.0",
  "main": ".eslintrc.js",
  "license": "ISC"
}
